datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}


model Product {
  id        Int     @id @default(autoincrement())
  title     String
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}